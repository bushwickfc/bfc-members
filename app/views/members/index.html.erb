<h1>Listing members</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Phone2</th>
      <th>Fax</th>
      <th>Address</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Zip</th>
      <th>Sex</th>
      <th>Status</th>
      <th>Join date</th>
      <th>Admin</th>
      <th>Membership agreement</th>
      <th>Monthly hours</th>
      <th>Fee discount</th>
      <th>Investment discount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.first_name %></td>
        <td><%= member.middle_name %></td>
        <td><%= member.last_name %></td>
        <td><%= member.email %></td>
        <td><%= member.phone %></td>
        <td><%= member.phone2 %></td>
        <td><%= member.fax %></td>
        <td><%= member.address %></td>
        <td><%= member.address2 %></td>
        <td><%= member.city %></td>
        <td><%= member.state %></td>
        <td><%= member.country %></td>
        <td><%= member.zip %></td>
        <td><%= member.sex %></td>
        <td><%= member.status %></td>
        <td><%= member.join_date %></td>
        <td><%= member.admin %></td>
        <td><%= member.membership_agreement %></td>
        <td><%= member.monthly_hours %></td>
        <td><%= member.fee_discount %></td>
        <td><%= member.investment_discount %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>

<div class="page-header">
<h1>Listing fees</h1>
</div>

<table>
  <thead>
    <tr>
      <th>Member</th>
      <th>receiver</th>
      <th>Amount</th>
      <th>Payment date</th>
      <th>Payment type</th>
      <th>Payment method</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fees.each do |fee| %>
      <tr>
        <td><%= fee.member.id %></td>
        <td><%= fee.receiver.id rescue nil %></td>
        <td><%= fee.amount %></td>
        <td><%= fee.payment_date %></td>
        <td><%= fee.payment_type %></td>
        <td><%= fee.payment_method %></td>
        <td><%= link_to 'Show', member_fee_path(@member, fee), role:'button', class:'btn btn-info'  %></td>
        <td><%= link_to 'Edit', edit_member_fee_path(@member, fee), role:'button', class:'btn btn-primary'  %></td>
        <td><%= link_to 'Destroy', member_fee_path(@member, fee), role:'button', class:'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fee', new_member_fee_path(@member), role:'button', class:'btn btn-success' %>

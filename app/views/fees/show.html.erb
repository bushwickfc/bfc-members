<%= render 'shared/csv' %>

<p>
  <strong>Member:</strong>
  <%= member_name(@fee, :member) %>
</p>

<p>
  <strong>Receiver:</strong>
  <%= member_name(@fee, :creator) %>
</p>

<p>
  <strong>Amount:</strong>
  <%= as_money @fee.amount %>
</p>

<p>
  <strong>Payment date:</strong>
  <%= @fee.payment_date %>
</p>

<p>
  <strong>Payment type:</strong>
  <%= @fee.payment_type %>
</p>

<p>
  <strong>Payment method:</strong>
  <%= @fee.payment_method %>
</p>

<%= link_to 'Edit', edit_fee_path(@fee), role:'button', class:'btn btn-primary' %>
<%= link_to 'Back', fees_path, role:'button', class:'btn btn-default'  %>
